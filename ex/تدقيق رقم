int? firstNonConsecutive(List<int> arr){
for (var i = 1; i < arr.length; i++) {
  if(arr[i]!=arr[i-1]+1){
return arr[i];
  } 
}
return null;
}
void main(List<String> args) {
  List <int> arr1=[1,2,3,4,5,6,7,9];
  List <int >arr2=[1,2,3,4,5,6,7,8,9];
  print(firstNonConsecutive(arr1));
  print(firstNonConsecutive(arr2));
}
